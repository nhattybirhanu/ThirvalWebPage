{
  "name": "thrival-web-page",
  "version": "0.0.0",
  "scripts": {
    "start-local": "ng serve",
    "ng": "ng",
    "build": "npm run build:ssr",
    "build:ssr": "ng build --configuration production && ng run thrival-web-page:server:production && ng run thrival-web-page:prerender && npm run serve:ssr",
    "serve:ssr": "node dist/thrival-web-page/server/main.mjs",
    "dev:ssr": "npm run build:ssr && npm run serve:ssr",
    "vercel-build": "npm run build:ssr",
    "start": "node dist/thrival-web-page/server/main.mjs",
    "heroku-postbuild": "npm run build:ssr",
    "local:ssr": "ng build --watch",
    "nodemon": "nodemon",
    "prerender": "ng run thrival-web-page:prerender",
    "deploy": "vercel --prod"
  }
,
  "private": true,
  "dependencies": {
