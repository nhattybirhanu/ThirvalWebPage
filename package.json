    "@angular/router": "^19.2.0",
    "@angular/ssr": "^19.2.6",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "bootstrap": "^5.3.5",
    "express": "^4.18.2",
    "extract-colors": "^4.2.0",
    "lottie-web": "5.9.2",
    "ngx-lottie": "^13.0.1",
