{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "handler": "dist/thrival-web-page/server/server.mjs",
        "runtime": "nodejs18.x",
        "environment": {
          "NODE_ENV": "production"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/thrival-web-page/browser/index.html"
    }
  ]
}
