{
  "version": 2,
  "buildCommand": "npm run build:ssr",
  "outputDirectory": "dist/thrival-web-page/browser",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.mjs"
    }
  ],
  "functions": {
    "api/index.mjs": {
      "runtime": "nodejs22.x",
      "includeFiles": "dist/thrival-web-page/**"
    }
  }
}
